<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="@dimen/headview"
                android:background="#FFFFFF">

        <LinearLayout
            android:id="@+id/back"
            android:layout_centerInParent="true"
            android:layout_alignParentLeft="true"
            android:layout_width="70dp"
            android:layout_height="match_parent">

            <ImageView
                style="@style/BACK_JIANTOU"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:src="@drawable/personmerchantsearchnetiddetailsback" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="返回"
                android:textColor="#333333"
                android:textSize="17sp" />
        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_height="match_parent">
            <ImageView
                android:id="@+id/iv_bank_icon"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/benefitthepeopleopenanaccounttopimg"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/tv_title"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
               android:layout_marginLeft="5dp"
                android:gravity="center"
                android:text="账户详情"
                android:textColor="#333333"
                android:textSize="@dimen/headviewtextsize"/>
    </LinearLayout>
</RelativeLayout>
