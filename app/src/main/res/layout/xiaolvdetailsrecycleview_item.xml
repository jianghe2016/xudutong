<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:orientation="vertical"
    android:textColor="@color/whiteblacktext">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_xiaolvdetailsrecycleveiw"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/xiaolvdetailsrecycleviewitem11"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingBottom="7dp"
            android:paddingLeft="15dp"
            android:paddingTop="7dp"
            android:textColor="@color/whitecolortext"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/xiaolvdetailsrecycleviewitem12"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_vertical|right"
            android:paddingBottom="7dp"
            android:paddingRight="15dp"
            android:paddingTop="7dp"
            android:textColor="@color/whitecolortext"
            android:textSize="13sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_xiaolvdetailsrecycleveiw2"
        android:orientation="horizontal"
        android:paddingTop="10dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:text="借车时间 : "
                android:textColor="#969696" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:text="还车时间 : "
                android:textColor="#969696" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:text="是否手工还车 : "
                android:textColor="#969696" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                android:id="@+id/xiaolvdetailsrecycleviewitem1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />

            <TextView
                android:id="@+id/xiaolvdetailsrecycleviewitem2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />

            <TextView
                android:id="@+id/xiaolvdetailsrecycleviewitem3"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />

            <TextView
                android:id="@+id/xiaolvdetailsrecycleviewitem4"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />

            <TextView
                android:id="@+id/xiaolvdetailsrecycleviewitem5"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:paddingLeft="15dp"
                android:textColor="#404040" />
        </LinearLayout>


    </LinearLayout>

</LinearLayout>
