<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/whitecolortext"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/headview"
        android:background="@color/whitecolortext">

        <LinearLayout
            android:id="@+id/funreturnsdetailedlayoutback"
            android:layout_width="70dp"
            android:layout_height="match_parent">

            <ImageView
                style="@style/BACK_JIANTOU"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:src="@drawable/personmerchantsearchnetiddetailsback" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="返回"
                android:textColor="@color/whitetopgraytext"
                android:textSize="17sp" />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="-70dp"
            android:gravity="center"
            android:text="收益明细"
            android:textColor="@color/whitetopgraytext"
            android:textSize="@dimen/headviewtextsize" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d6d6" />

    <LinearLayout
        android:id="@+id/funreturnsdetailedlayoutbacktopview1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:background="@color/whitecolortext"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="累计收益"
            android:textColor="#1d1d1d"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/funreturnsdetailedlayoutbacktopviewimg11"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/funds1layoutsanjiao" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#d8d6d6" />

    <TextView
        android:id="@+id/funreturnsdetailedlayoutpopwindowitem1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="万份收益"
        android:textColor="#525252"
        android:textSize="17sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:background="#eeeeee" />

    <TextView
        android:id="@+id/funreturnsdetailedlayoutpopwindowitem2"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="累计收益"
        android:textColor="#525252"
        android:textSize="17sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:background="#eeeeee" />

    <TextView
        android:id="@+id/funreturnsdetailedlayoutpopwindowitem3"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="近一周收益"
        android:textColor="#525252"
        android:textSize="17sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:background="#eeeeee" />

    <TextView
        android:id="@+id/funreturnsdetailedlayoutpopwindowitem4"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="近一月收益"
        android:textColor="#525252"
        android:textSize="17sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:background="#eeeeee" />
</LinearLayout>